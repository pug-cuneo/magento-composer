{
    "name": "pug-cuneo/magento-composer",
    "description": "Install Magento via Composer",
    "minimum-stability": "dev",
    "type": "magento-module",
    "repositories":[
        { "type": "composer", "url": "https://packages.firegento.com" }
    ],
    "require":{
        "magento-hackathon/magento-composer-installer": "3.0.7",
        "magento/core": "1.9.3.2",
        "aydin-hassan/magento-core-composer-installer": "^1.4"
    },
    "extra":{
        "magento-root-dir": "web",
        "magento-deploystrategy": "symlink",
        "magento-deploystrategy-overwrite": {
            "magento/core": "copy"
        },
        "magento-force": true
    }
}
